<Files .htaccess>
order allow,deny
deny from all
</Files>


Options +FollowSymLinks
RewriteEngine On

# force www. in all requests
RewriteCond %{HTTP_HOST} ^ojee\.pk [NC]
RewriteRule ^(.*)$ http://www.ojee.pk/$1 [L,R=301]

# enable hiding html extension
RewriteCond %{REQUEST_FILENAME} !-d


RewriteEngine on

RewriteRule ^([A-Za-z0-9-]+)/?$ deal.php?title=$1 [NC,L]

